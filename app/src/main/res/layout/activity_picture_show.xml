<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="15dp"
    tools:context=".PictureShow">

    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="10dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/afterbinary"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/afterbinary"
        android:id="@+id/sayafter"
        android:gravity="center"
        android:text="@string/afterbinary"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/sayafter"
        android:layout_margin="10dp"
        android:gravity="center_horizontal"
        android:textColor="@color/colorAccent"
        android:textSize="20sp" />



    <ImageView
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_margin="5dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/result"
        android:id="@+id/temp1"/>
    <ImageView
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_margin="5dp"
        android:layout_toRightOf="@id/temp1"
        android:layout_below="@id/result"
        android:id="@+id/temp2"/>
    <ImageView
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="5dp"
        android:layout_toRightOf="@id/temp2"
        android:layout_below="@id/result"
        android:id="@+id/temp3"/>
    <ImageView
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="5dp"
        android:layout_toRightOf="@id/temp3"
        android:layout_below="@id/result"
        android:id="@+id/temp4"/>
    <ImageView
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="5dp"
        android:layout_toRightOf="@id/temp4"
        android:layout_below="@id/result"
        android:id="@+id/temp5"/>
    <ImageView
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="5dp"
        android:layout_toRightOf="@id/temp5"
        android:layout_below="@id/result"
        android:id="@+id/temp6"/>
    <ImageView
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="5dp"
        android:layout_toRightOf="@id/temp6"
        android:layout_below="@id/result"
        android:id="@+id/temp7"/>
    <ImageView
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:layout_gravity="center_horizontal"
        android:layout_margin="5dp"
